FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform $turbulentOmega;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    ahmed_front
	{
        type            omegaWallFunction;
        value           $internalField;
	}

	ahmed_sides
	{
        type            omegaWallFunction;
        value           $internalField;
	}

	ahmed_top
	{
        type            omegaWallFunction;
        value           $internalField;
	}

	ahmed_bottom
	{
        type            omegaWallFunction;
        value           $internalField;
	}

	ahmed_slant
	{
        type            omegaWallFunction;
        value           $internalField;
	}

	ahmed_rear
	{
        type            omegaWallFunction;
        value           $internalField;
	}

	inlet
	{
        type            fixedValue;
        value           $internalField;
	}

	side_right
	{
        type            slip;
	}

	side_left
	{
        type            slip;
	}

	outlet
	{
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
	}

	top
	{
        type            slip;
	}

	bottom
	{
        type            omegaWallFunction;
        value           $internalField;
	}


}


