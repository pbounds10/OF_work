skinFrictionCo1
{

    type            skinFrictionCo;
    libs            ("libfieldFunctionObjects_usr.so");

    patches ($keyPatches);

    rho             rhoInf;      // Indicates incompressible
    rhoInf          $rhoRef;           // Required when rho = rhoInf
